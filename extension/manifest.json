{
    "manifest_version": 2,
    "name": "Self Stopper",
    "author": "Pouya Kary",
    "incognito": "spanning",
    "version": "0.5",
    "description": "Filtering things to myself",
    "icons": {
        "48": "icon/icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://www.instagram.com/*",
                "*://twitter.com/*",
                "*://dribbble.com/*",
                "*.//pinterest.com/*"
            ],
            "js": [
                "block.js"
            ]
        }
    ]
}
